{
  "name": "flash-card",
  "version": "1.0.0",
  "description": "My outstanding Nuxt.js project",
  "author": "pb10001",
  "private": true,
  "scripts": {
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "dev": "npm-run-all --parallel dev:**",
    "dev:app": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev:functions": "netlify-lambda serve src -c ./webpack.config.js",
    "build": "npm-run-all --parallel build:**",
    "build:app": "nuxt build",
    "build:functions": "netlify-lambda build src -c ./webpack.config.js",
    "start": "npm-run-all --parallel start:**",
    "start:app": "cross-env NODE_ENV=production node server/index.js",
    "start:functions": "netlify-lambda serve src -c ./webpack.config.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "prebuild": "npm run bootstrap",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.2",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/vuetify": "^0.5.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.3",
    "faunadb": "^2.6.0",
    "gotrue-js": "^0.9.24",
    "netlify-identity-widget": "^1.5.2",
    "nuxt": "^2.4.0",
    "nuxt-buefy": "^0.3.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^23.6.0",
    "netlify-lambda": "^1.4.2",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "1.14.3",
    "vue-jest": "^3.0.2"
  }
}
